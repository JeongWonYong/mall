<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mall.admin.login.mapper.AdminLoginDao">

	<cache />
	
	<select id="login" parameterType="com.mall.admin.member.entity.AdminMemberEntity"
		resultType="java.lang.String">
		select adm_mem_pw
		from ta_member
		where adm_mem_id = #{adminID}
	</select>


</mapper>
